{
	"name": "trafikode",
	"displayName": "trafikode",
	"description": "traefik auto complete",
	"version": "0.0.1",
	"author": {
		"name": "Hapsida @securisec"
	},
	"publisher": "securisec",
	"repository": {
		"url": "https://github.com/securisec/traefikode"
	},
	"engines": {
		"vscode": "^1.42.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onLanguage:yaml",
		"onLanguage:dockerfile"
	],
	"contributes": {
		"languages": [
			{
				"id": "dockerfile",
				"aliases": [
					"Dockerfile"
				],
				"filenamePatterns": [
					"*.dockerfile",
					"Dockerfile",
					"Dockerfile.debug",
					"Dockerfile.dev",
					"Dockerfile.develop",
					"Dockerfile.prod"
				]
			},
			{
				"id": "yaml",
				"aliases": [
					"docker-compose"
				],
				"filenamePatterns": [
					"docker-compose.yaml",
					"docker-compose.yaml",
					"docker-compose.*.yaml",
					"docker-compose.*.yml",
					"traefik.yml",
					"traefik.yaml"
				]
			}
		],
		"configurationDefaults": {
			"[yaml]": {
				"editor.quickSuggestions": {
					"other": true,
					"comments": false,
					"strings": true
				}
			},
			"[dockerfile]": {
				"editor.quickSuggestions": {
					"other": true,
					"comments": false,
					"strings": true
				}
			}
		}
	},
	"main": "./out/extension.js",
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint src --ext ts",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^7.0.1",
		"@types/node": "^12.11.7",
		"@types/vscode": "^1.42.0",
		"eslint": "^6.8.0",
		"@typescript-eslint/parser": "^2.18.0",
		"@typescript-eslint/eslint-plugin": "^2.18.0",
		"glob": "^7.1.6",
		"mocha": "^7.0.1",
		"typescript": "^3.7.5",
		"vscode-test": "^1.3.0"
	}
}
