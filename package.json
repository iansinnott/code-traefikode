{
	"name": "traefikode",
	"displayName": "traefikode",
	"description": "traefik auto complete",
	"version": "0.0.11",
	"icon": "./resources/traefik.square.png",
	"author": {
		"name": "Hapsida @securisec"
	},
	"contributors": [
		{
			"name": "Ian Sinnott",
			"url": "https://github.com/iansinnott"
		}
	],
	"publisher": "iansinnott",
	"repository": {
		"url": "https://github.com/iansinnott/code-traefikode"
	},
	"engines": {
		"vscode": "^1.42.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onLanguage:yaml"
	],
	"contributes": {
		"languages": [
			{
				"id": "yaml",
				"aliases": [
					"docker-compose"
				],
				"filenamePatterns": [
					"docker-compose.yaml",
					"docker-compose.yaml",
					"docker-compose*.yaml",
					"docker-compose*.yml"
				]
			}
		],
		"configurationDefaults": {
			"[yaml]": {
				"editor.quickSuggestions": {
					"other": true,
					"comments": false,
					"strings": true
				}
			}
		}
	},
	"main": "./out/extension.js",
	"scripts": {
		"build": "yarn compile",
		"package": "yarn build && vsce package",
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint src --ext ts",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^7.0.1",
		"@types/node": "^12.11.7",
		"@types/vscode": "^1.42.0",
		"@typescript-eslint/eslint-plugin": "^2.18.0",
		"@typescript-eslint/parser": "^2.18.0",
		"glob": "^7.1.6",
		"typescript": "^3.7.5",
		"vsce": "^1.85.0",
		"vscode-test": "^1.3.0"
	}
}
